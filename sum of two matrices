// C program to find sum of two matrices
#include<stdio.h>
int main()
{
    int a[3][2], b[3][2],c[3][2], i,j;
    printf("Enter the elements of First matrix:");
    for(i=0;i<3;i++)
    {
        for(j=0;j<2;j++)
        scanf("%d",&a[i][j]);
    }
    printf("Enter the elements of the second matrix:");
    for(i=0;i<3;i++)
    {
        for(j=0;j<2;j++)
        scanf("%d",&b[i][j]);
    }
    for(i=0;i<3;i++)
    {
        for(j=0;j<2;j++)
        c[i][j]=a[i][j]+b[i][j];
    }
    printf("Sum of two Matrices\n");
    for(i=0;i<3;i++)
    {
        for(j=0;j<2;j++)
        printf("%d\t",c[i][j]);
        printf("\n");
    }
    return 0;
}
